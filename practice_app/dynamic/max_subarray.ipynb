   "source": [
    "def max_subarray(nums):\n",
    "    cur_sub = max_sub = nums[0]\n",
    "    \n",
    "    for num in nums[1:]:\n",
    "        cur_sub = max(num, cur_sub+num)\n",
    "        max_sub = max(cur_sub, max_sub)\n",
    "        \n",
    "    return max_sub"
   ]
  },
