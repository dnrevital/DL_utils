    "def max_subarray(nums):\n",
    "    size = len(nums)\n",
    "    sums = [[0 for j in range(size)] for i in range(size)]\n",
    "    \n",
    "    for i in range(size):\n",
    "        sums[i][i] = nums[i]\n",
    "    \n",
    "    for i in range(size):\n",
    "        for j in range(i+1, size):\n",
    "            sums[i][j] = sums[i][j-1] + nums[j]\n",
    "            \n",
    "    print(f'sums: {sums}')"
   ]
