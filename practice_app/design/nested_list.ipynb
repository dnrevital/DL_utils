   "source": [
    "def flatten(nested):\n",
    "    flat = []\n",
    "    \n",
    "    for n in nested:\n",
    "        if type(n) == int:\n",
    "            flat += [n]\n",
    "        else:\n",
    "            flat += flatten(n)\n",
    "            \n",
    "    return flat"
   ]
  },
